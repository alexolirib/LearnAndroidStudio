<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AtmEmpresaConsultoriaActivity">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:background="@drawable/logo_atm"
        />

    <ImageView
        android:id="@+id/imgAtmEmpresaId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="47dp"
        android:layout_marginTop="160dp"
        android:background="@drawable/menu_empresa" />

    <ImageView
        android:id="@+id/imgAtmServicosId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/imgAtmEmpresaId"
        android:layout_marginEnd="43dp"
        android:background="@drawable/menu_servico" />

    <ImageView
        android:id="@+id/imgAtmClientesId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/imgAtmEmpresaId"
        android:layout_marginBottom="56dp"
        android:background="@drawable/menu_cliente" />

    <ImageView
        android:id="@+id/imgAtmContatoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imgAtmServicosId"
        android:layout_alignTop="@+id/imgAtmClientesId"
        android:background="@drawable/menu_contato" />
</android.widget.RelativeLayout>